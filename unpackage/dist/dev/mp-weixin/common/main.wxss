page {
	font-family: PingFang SC, 'Helvetica Neue', Arial, sans-serif;
	background-color: #f7f8fa;
	color: #353535;
	font-size: 14pt;
}
.red {
	color: #e64340;
}
.iconfont {
	font-size: 20pt;
}
.footer {
	text-align: center;
	width: 100%;
}
.footer-btn {
	width: 50%;
	color: #fff;
	background: #fd746c;
}
.panel {
	margin-bottom: 10px;
}
.panel-cell {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	width: 100%;
	height: 120rpx;
	background: #fff;
}
.panel-title {
	width: 100%;
	box-sizing: border-box;
	padding: 20rpx;
	font-size: 16pt;
}
.panel-cell-hd {
	width: 10%;
	padding: 0 12rpx;
	text-align: center;
}
.panel-cell-bd {
	width: 90%;
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	box-sizing: border-box;
	padding-right: 12px;
	border-bottom: 1px solid #f7f8fa;
}
.panel-cell--center {
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.panel-cell-text {
	margin-right: 10rpx;
	vertical-align: text-top;
}

