.container{padding:0 10rpx;padding-bottom:80px}.coin-box{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center;box-sizing:border-box;width:220rpx;height:260rpx;margin-bottom:20rpx;border-radius:6px;box-shadow:0 4px 9px 0 rgba(109,107,107,.5);position:relative}.grayfilter{-webkit-filter:grayscale(100%);filter:grayscale(100%)}.coin-lock{position:absolute;top:50%;left:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%);width:100rpx;height:100rpx;border-radius:50%;background-color:rgba(0,0,0,.6);display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;align-items:center}.icon-suoding{color:#fff;font-size:20px}.selectActive{border-radius:10px;box-shadow:#fd746c 0 10px 20px 0,inset #fd746c 0 0 0 2px,inset hsla(0,0%,100%,.5) 0 0 0 3px}.coin-img{width:120rpx;height:120rpx}.coin-spec{color:#ccc;font-size:13pt}