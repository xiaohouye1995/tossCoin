<view class="panel"><view class="panel-title"><text>十二生肖系列</text></view><view class="cell"><block wx:for="{{coins}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectCoin',['$0',index],[[['coins','',index]]]]]]]}}" class="{{['coin-box',(coinIndex===index)?'selectActive':'']}}" bindtap="__e"><image class="coin-img" src="{{item.src}}" mode></image><text>{{item.name}}</text><block wx:if="{{item.id===coinName}}"><text class="coin-spec">使用中</text></block></view></block></view><block wx:if="{{coinIndex!==-1}}"><view class="footer"><button class="footer-btn" style="background:#fd746c;" type="primary" data-event-opts="{{[['tap',[['setCoin']]]]}}" bindtap="__e">立即使用</button></view></block></view>