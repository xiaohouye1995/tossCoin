<view class="container"><block tt:for="{{coins}}" tt:for-item="item" tt:for-index="__i0__" tt:key="guid"><view class="panel"><view class="panel-title"><text>{{item.name}}</text></view><view class="cell"><block tt:for="{{item.list}}" tt:for-item="item2" tt:for-index="__i1__" tt:key="id"><view data-event-opts="{{[['tap',[['selectCoin',['$0'],[[['coins','guid',item.guid],['list','id',item2.id]]]]]]]}}" class="{{((('coin-box')+' '+((coinIndex===item2.id)?'selectActive':''))+' '+((!item2.status)?'grayfilter':''))}}" bindtap="__e"><easy-loadimage class="coin-img" vue-id="{{'fda6d982-1-'+__i0__+'-'+__i1__}}" mode="widthFix" scroll-top="{{scrollTop}}" image-src="{{item2.src}}" bind:__l="__l"></easy-loadimage><text>{{item2.name}}</text><block tt:if="{{item2.value===coinName}}"><text class="coin-spec">使用中</text></block><block tt:if="{{!item2.status}}"><view class="coin-lock"><text class="iconfont icon-suoding"></text></view></block></view></block></view></view></block><block tt:if="{{coinIndex!==-1}}"><view class="footer"><block tt:if="{{lockStatus}}"><button data-event-opts="{{[['tap',[['setCoin']]]]}}" class="footer-btn" bindtap="__e">立即使用</button></block><block tt:if="{{!lockStatus}}"><button data-event-opts="{{[['tap',[['unlockCoin']]]]}}" class="footer-btn" bindtap="__e">解锁并使用</button></block></view></block></view>